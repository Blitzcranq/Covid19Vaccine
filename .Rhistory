# Read the CSV file
data <- read.csv("trentdata.csv", header = FALSE, stringsAsFactors = FALSE)
# Identify rows with relevant information (e.g., "Year" row)
header_rows <- grep("Year", data$X)
header_rows <- header_rows[1]  # Use the first occurrence
# Transpose the data for better handling
data <- t(data)
# Set column names based on the identified header row
colnames(data) <- data[header_rows, ]
# Remove unnecessary rows and columns
data <- data[(header_rows + 1):nrow(data), -1]
# Convert data to numeric (excluding the first column)
data[, -1] <- sapply(data[, -1], as.numeric)
# Create pie charts for 2022, 2017, and 2012
years <- c(2022, 2017, 2012)
for (year in years) {
# Extract data for the specific year
enrollment_data <- data[, c("Year", as.character(year))]
# Remove rows with NA values
enrollment_data <- enrollment_data[complete.cases(enrollment_data), ]
# Create a pie chart
pie(enrollment_data[, 2], labels = enrollment_data[, 1], main = paste("Enrollment in", year))
}
